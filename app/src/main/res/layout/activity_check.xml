<?xml version="1.0" encoding="utf-8"?>
<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
            xmlns:tools="http://schemas.android.com/tools"
            xmlns:app="http://schemas.android.com/apk/res-auto"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
>
<android.support.constraint.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".Check" android:background="@color/a">

    <android.support.constraint.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="@drawable/for_settings" app:layout_constraintTop_toTopOf="parent"
            android:id="@+id/constraintLayout2">

        <TextView android:layout_width="wrap_content"
                  android:layout_height="wrap_content"
                  android:textSize="25sp"
                  android:textColor="@color/white"
                  android:textStyle="bold"
                  android:id="@+id/textView144"
                  android:text="CheckOut"
                  android:layout_marginTop="58dp"
                  app:layout_constraintTop_toTopOf="parent"
                  app:layout_constraintStart_toStartOf="parent" app:layout_constraintHorizontal_bias="0.5"
                  app:layout_constraintEnd_toEndOf="parent"
        />
        <Button
                android:layout_width="16dp"
                android:layout_height="16dp"
                android:id="@+id/finish_step1"
                android:background="@drawable/left_back" app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintStart_toStartOf="parent" android:layout_marginTop="60dp"
                android:layout_marginStart="20dp"/>
        <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" app:srcCompat="@drawable/first_step"
                android:id="@+id/imageView21"
                app:layout_constraintStart_toStartOf="parent" app:layout_constraintHorizontal_bias="0.5"
                app:layout_constraintEnd_toEndOf="parent" app:layout_constraintTop_toBottomOf="@+id/textView144"
                android:layout_marginTop="36dp" app:layout_constraintBottom_toBottomOf="parent"
                android:layout_marginBottom="61dp"/>
    </android.support.constraint.ConstraintLayout>
    <android.support.constraint.ConstraintLayout
            android:focusable="false"
            android:focusableInTouchMode="true"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="184dp" app:layout_constraintTop_toTopOf="@+id/constraintLayout2"
            android:background="@drawable/for_home_categories" app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginEnd="10dp"
            android:layout_marginStart="10dp" android:layout_marginBottom="110dp"
            app:layout_constraintBottom_toBottomOf="parent">

        <TextView
                android:text="Contact Info"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" android:id="@+id/textView76"
                app:layout_constraintTop_toTopOf="parent" app:layout_constraintStart_toStartOf="parent"
                android:layout_marginTop="27dp" android:layout_marginStart="14dp" android:textSize="18sp"
                android:textColor="@color/darkGreen"/>
        <TextView
                android:text="Full Name"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" android:id="@+id/textView77" android:textColor="@color/Black"
                app:layout_constraintStart_toStartOf="parent"
                android:layout_marginStart="14dp" android:layout_marginTop="24dp"
                app:layout_constraintTop_toBottomOf="@+id/textView76"/>
        <EditText
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:inputType="textPersonName"
                android:hint="Full Name"
                android:ems="10"
                android:id="@+id/editText16"
                app:layout_constraintStart_toStartOf="parent" android:layout_marginStart="14dp"
                android:layout_marginTop="7dp" app:layout_constraintTop_toBottomOf="@+id/textView77"
                android:background="@color/white" android:textColorHint="@color/editGray" android:paddingTop="14dp"
                android:paddingBottom="14dp" app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="14dp"
                android:paddingLeft="20dp"/>
        <TextView
                android:text="Phone"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" android:id="@+id/textView78"
                android:layout_marginTop="18dp"
                app:layout_constraintTop_toBottomOf="@+id/editText16" app:layout_constraintStart_toStartOf="parent"
                android:layout_marginStart="14dp" android:textColor="@color/Black"/>
        <EditText
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:inputType="textPersonName"
                android:hint="996 XXXX XXXX"
                android:ems="10"
                android:id="@+id/editText17"
                app:layout_constraintTop_toBottomOf="@+id/textView78"
                android:layout_marginTop="7dp" app:layout_constraintStart_toStartOf="parent"
                android:layout_marginStart="14dp" app:layout_constraintEnd_toEndOf="parent"
                android:layout_marginEnd="188dp" android:background="@color/white" android:paddingTop="14dp"
                android:paddingBottom="14dp" android:paddingLeft="20dp" android:textColorHint="@color/editGray"/>
        <TextView
                android:text="Email"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" android:id="@+id/textView79"
                app:layout_constraintTop_toBottomOf="@+id/editText16"
                android:layout_marginTop="18dp"
                app:layout_constraintStart_toEndOf="@+id/textView78" android:layout_marginStart="140dp"
                android:textColor="@color/Black"/>
        <EditText
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:inputType="textPersonName"
                android:hint="Email"
                android:ems="10"
                android:id="@+id/editText18" android:background="@color/white" android:textColorHint="@color/editGray"
                app:layout_constraintStart_toStartOf="parent" android:layout_marginStart="195dp"
                app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="20dp" android:paddingTop="14dp"
                android:paddingBottom="14dp" android:paddingLeft="20dp" android:layout_marginTop="7dp"
                app:layout_constraintTop_toBottomOf="@+id/textView79"/>
        <TextView
                android:text="Adress Details"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" android:id="@+id/textView80"
                app:layout_constraintStart_toStartOf="parent" android:layout_marginStart="14dp"
                android:layout_marginTop="18dp" app:layout_constraintTop_toBottomOf="@+id/editText17"
                android:textColor="@color/Black"/>
        <EditText
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:inputType="textPersonName"
                android:hint="Like Dubai , Emara Street .."
                android:ems="10"
                android:id="@+id/editText19"
                android:textColorHint="@color/editGray"
                android:background="@color/white" app:layout_constraintStart_toStartOf="parent"
                android:layout_marginStart="14dp" app:layout_constraintTop_toBottomOf="@+id/textView80"
                android:layout_marginTop="7dp" app:layout_constraintEnd_toEndOf="parent"
                android:layout_marginEnd="14dp" android:paddingLeft="15dp" android:paddingTop="18dp"
                android:paddingBottom="110dp"/>
        <Button
                android:text="Next"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:id="@+id/next_to_step2"
                android:layout_marginTop="35dp"
                app:layout_constraintTop_toBottomOf="@+id/editText19" app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintEnd_toEndOf="parent" android:layout_marginStart="14dp"
                android:layout_marginEnd="14dp"
                android:background="@drawable/for_buttons"
                android:textColor="@color/white" android:textAllCaps="false"
                app:layout_constraintBottom_toBottomOf="parent" android:layout_marginBottom="35dp"/>
    </android.support.constraint.ConstraintLayout>

</android.support.constraint.ConstraintLayout>
</ScrollView>